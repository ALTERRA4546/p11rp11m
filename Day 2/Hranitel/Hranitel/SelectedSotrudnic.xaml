<Window x:Class="Hranitel.SelectedSotrudnic"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hranitel"
        mc:Ignorable="d"
        Title="Выбор сотрудника" Height="117" Width="598">
    <Grid>
        <ComboBox x:Name="selectSotrud" HorizontalAlignment="Left" Margin="10,9,0,0" VerticalAlignment="Top" Width="566" FontSize="18" RenderTransformOrigin="0.5,0.5">
        </ComboBox>
        <Button x:Name="enter" Content="Выбрать" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Width="566" FontSize="18" Click="enter_Click"/>

    </Grid>
</Window>
